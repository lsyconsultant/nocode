<mvc:View
        controllerName="sap.ui.nocode.controller.framework.projectSetting"
        xmlns="sap.m"
        xmlns:mvc="sap.ui.core.mvc"
        xmlns:tnt="sap.tnt"
        xmlns:f="sap.ui.layout.form"
        xmlns:core="sap.ui.core">

    <VBox justifyContent="Center" alignItems="Center" height="500px" width="100%">
        <f:Form id="FormDisplay354"
                editable="false">
            <f:title>
                <core:Title text="노코드 플렛폼 시작을 환영합니다. 화이팅!!!"/>
            </f:title>
            <f:layout>
                <f:ResponsiveGridLayout
                        labelSpanXL="3"
                        labelSpanL="3"
                        labelSpanM="3"
                        labelSpanS="12"
                        adjustLabelSpan="false"
                        emptySpanXL="4"
                        emptySpanL="4"
                        emptySpanM="4"
                        emptySpanS="0"
                        columnsXL="1"
                        columnsL="1"
                        columnsM="1"
                        singleContainerFullSize="false"/>
            </f:layout>
            <f:formContainers>
                <f:FormContainer>
                    <f:formElements>
                        <f:FormElement label="프로젝트 Id">
                            <f:fields>
                                <Input id="projectId"/>
                            </f:fields>
                        </f:FormElement>
                        <f:FormElement label="프로젝트 이름">
                            <f:fields>
                                <Input id="projectNm"/>
                            </f:fields>
                        </f:FormElement>
                        <f:FormElement label="프로젝트 생성 위치">
                            <f:fields>
                                <Input id="projectPath"/>
                            </f:fields>
                        </f:FormElement>
                    </f:formElements>
                </f:FormContainer>
            </f:formContainers>
        </f:Form>
        <Button text="생성" type="Emphasized" press="onProjectCreate"/>
    </VBox>


</mvc:View>
